<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!-- Alteration business object -->
    <bean id="tagBo" class="org.mskcc.cbio.oncokb.bo.impl.TagBoImpl" >
   		<property name="dao" ref="tagDao" />
    </bean>
 
    <!-- Alteration Data Access Object -->
    <bean id="tagDao" class="org.mskcc.cbio.oncokb.dao.impl.TagDaoImpl" >
   		<property name="sessionFactory" ref="sessionFactory"></property>
    </bean>
   
   		
</beans>